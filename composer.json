{
    "name": "teamnovu/nova-external-url",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "pixelfear/composer-dist-plugin": "^0.1.5"
    },
    "autoload": {
        "psr-4": {
            "Teamnovu\\NovaExternalUrl\\": "src/"
        }
    },
    "extra": {
        "download-dist": [
            {
                "url": "https://github.com/teamnovu/teamnovu/nova-external-url/releases/download/{$version}/dist.tar.gz",
                "path": "vendor/teamnovu/nova-external-url/dist"
            }
        ],
        "laravel": {
            "providers": [
                "Teamnovu\\NovaExternalUrl\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pixelfear/composer-dist-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
